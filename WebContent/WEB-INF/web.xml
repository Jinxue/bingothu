<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Bingo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>search.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>SearchProcess</display-name>
    <servlet-name>SearchProcess</servlet-name>
    <servlet-class>Bingo.servlet.SearchProcess</servlet-class>
    <init-param>
    	<param-name>indexDir</param-name>
    	<param-value>/index/</param-value>
    </init-param>
    <init-param>
    	<param-name>dataDir</param-name>
    	<param-value>/data/</param-value>
    </init-param>
    
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchProcess</servlet-name>
    <url-pattern>/SearchProcess</url-pattern>
  </servlet-mapping>
</web-app>